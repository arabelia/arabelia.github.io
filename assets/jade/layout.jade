include mixin.jade
doctype html
html(lang='es-PE')
	include head.jade
	body.centrar(onload="mueveReloj()")
		.contenedor
			//-header
			.container
				.row.espaciado
					.col-sm-4
						.foto1.rotar
							img(src='../images/foto2.jpg')
					.col-sm-8
						block conoceme
				.row.espaciado
					block tituloIntereses
					+separador()
					block intereses
						.row
							.col-xs-4
								.interes1
									block interes1
							.col-xs-4
								.interes2
									block interes2
									//- reloj
									//- form(name="form_reloj") 
									//- 	input(type="text" name="reloj" size="10")
									#reloj
							.col-xs-4
								.interes3
									block interes3
						.row
							.interes4
								block interes4
							.interes5
								block interes5
						.row
							.interes6
								block interes6
					+separador()
				.row.espaciado
					block trabajos
					block cuadros
				.row.contactos.espaciado
					block contact0
						.enlinea.tam1
							+imagen('../images/correo.png')
						.enlinea
							block contactame
						+separador()
						p.correo.tblanco.courier arabelia.aragon.q@gmail.com
						.contacto.enlinea
							+imagen('../images/face.jpg')
						.contacto.enlinea
							+imagen('../images/github.png')
		script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js")
		script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous")
		script(type="text/javascript" src="../js/main.js")
